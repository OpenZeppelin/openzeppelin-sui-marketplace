{
  "name": "sui-oracle-market",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "description": "Scripts and tooling for deploying the Sui Oracle Market Move package.",
  "scripts": {
    "start:localnet": "NODE_OPTIONS=\"--no-warnings\" ts-node-esm --transpile-only src/scripts/chain/start-localnet.ts",
    "publish:package": "NODE_OPTIONS=\"--no-warnings\" ts-node-esm --transpile-only src/scripts/chain/publish-package.ts",
    "setup:local": "NODE_OPTIONS=\"--no-warnings\" ts-node-esm --transpile-only src/scripts/chain/setup-local.ts",
    "get:mock-currency": "NODE_OPTIONS=\"--no-warnings\" ts-node-esm --transpile-only src/scripts/chain/get-mock-currency.ts",
    "get:address-info": "NODE_OPTIONS=\"--no-warnings\" ts-node-esm --transpile-only src/scripts/chain/get-address-info.ts",
    "get:object-info": "NODE_OPTIONS=\"--no-warnings\" ts-node-esm --transpile-only src/scripts/chain/get-object-info.ts",
    "stop:localnet": "NODE_OPTIONS=\"--no-warnings\" ts-node-esm --transpile-only src/scripts/chain/stop-localnet.ts",
    "owner:create-shop": "NODE_OPTIONS=\"--no-warnings\" ts-node-esm --transpile-only src/scripts/owner/create-shop.ts",
    "owner:add-item": "NODE_OPTIONS=\"--no-warnings\" ts-node-esm --transpile-only src/scripts/owner/add-item.ts",
    "lint": "pnpm exec eslint ."
  },
  "dependencies": {
    "@mysten/sui": "^1.45.0",
    "chalk": "^5.3.0",
    "dotenv": "^17.2.3",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.19.2",
    "@types/yargs": "^17.0.35",
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unicorn": "^62.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0"
  }
}